<div class="beers-container">
     {{#each beerList}}
     <div class="card">
          <img class="card-img-top" style="width: 8rem;" src="{{this.image_url}}" alt="">
          <div class="card-body">
               <h5 class="card-title">{{this.tagline}}</h5>
               <p class="card-text">{{this.name}}</p>
               <p class="card-text">{{this.description}}</p>
          </div>
     </div>
     {{/each}}
</div>